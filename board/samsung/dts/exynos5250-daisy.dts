/dts-v1/;
/include/ "exynos5250.dtsi"
/include/ "flashmap-exynos-ro.dtsi"
/include/ "flashmap-4mb-rw.dtsi"
/include/ "chromeos-exynos.dtsi"
/include/ "cros5250-common.dtsi"

/ {
        model = "Google Daisy";
        compatible = "google,daisy", "samsung,smdk5250", "samsung,exynos5250";

	sromc@12250000 {
		bank = <3>;
		srom-timing = <1 9 12 1 6 1 1>;
		width = <2>;
		lan@7000000 {
			compatible = "smsc,lan9215", "smsc,lan";
			reg = <0x7000000 0x100>;
			phy-mode = "mii";
		};
	};

	i2c@12c90000 {
		tpm@20 {
			reg = <0x20>;
			compatible = "infineon,slb9635-tpm";
		};
	};

	spi@131b0000 {
		mkbp@0 {
			reg = <0>;
			compatible = "google,mkbp";
			spi-max-frequency = <10000000>;
		};
	};

};
